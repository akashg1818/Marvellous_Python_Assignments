class Product:

    def __init__(self,A,B):
        self.Name = A
        self.Price = B

    def Display(self):
        print("Product name is :"+self.Name)
        print("Price of product is :",self.Price)



def main():

    obj1 = Product("Watch",1000)

    obj1.Display()


if __name__ == "__main__":
    main()
